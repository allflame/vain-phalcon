    {
      "type": "git",
      "url": "http://gitlab.cosmonova.net.ua/mmjitsu/vain-logger.git"
    },
    {
      "type": "git",
      "url": "http://gitlab.cosmonova.net.ua/mmjitsu/vain-database.git"
    },
    {
      "type": "git",
      "url": "http://gitlab.cosmonova.net.ua/mmjitsu/vain-operation.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/allflame/vain-event"
    },
    {
      "type": "vcs",
      "url": "https://github.com/allflame/vain-entity"
    }
  ],
  "config": {
    "secure-http": false
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
